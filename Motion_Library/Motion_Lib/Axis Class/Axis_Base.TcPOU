    <Declaration><![CDATA[FUNCTION_BLOCK Axis_Base IMPLEMENTS I_Cyclic, I_AxisControl, I_AxisStatus, I_Resettable
VAR_INPUT
END_VAR
VAR_OUTPUT
END_VAR
VAR
	_axis : REFERENCE TO Tc2_MC2.AXIS_REF;
	axesEnable : AxisPower(_axis);
	axesResetCommand : AxisReset(_axis);
	axesMoveToCommand : BufferedMoveToPosition(_axis);
	axesHaltCommand	: AxisHalt(_axis);
	axesReadError : Tc2_MC2.MC_ReadAxisError;
	_MoverErrorId: DWORD;
	_Logger : TcMEssage_Ext_Null;
	_TraceLogger : ITcMessage_Ext := _Logger;
	CurrentTask : I_Task;
END_VAR]]></Declaration>
    <Implementation>
      <ST><![CDATA[{analysis -2}]]></ST>
    </Implementation>
