VAR_INPUT
	Distance : MC_LREAL;
	Velocity : MC_LREAL;
END_VAR

]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF ABS(Distance) > 0 THEN
	DefaultRelBehaviour.MoveBy(Distance,Velocity);
	MoveByDistance := 1;
ELSE
	MoveByDistance := -1;
END_IF

]]></ST>
