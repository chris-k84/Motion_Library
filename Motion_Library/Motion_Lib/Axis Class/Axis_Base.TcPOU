      </Implementation>
    </Method>
    <Method Name="HaltAxis" Id="{b5b4d676-a559-4d25-8571-0c2ec9c3c015}" FolderPath="Commands\">
      <Declaration><![CDATA[METHOD PUBLIC HaltAxis : HRESULT
VAR
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF NOT axesHaltCommand.Busy THEN
	HaltAxis := axesHaltCommand.Execute(CurrentTask);
	axesMoveToCommand.Abort();
ELSE 
	HaltAxis := -1;
END_IF]]></ST>
      </Implementation>
    </Method>
