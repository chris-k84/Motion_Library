    <Method Name="GearIn" Id="{a71fc049-0a78-4bbd-8c6d-e1cd8c076d5d}">
      <Declaration><![CDATA[METHOD GearIn : HRESULT
VAR_INPUT
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF _AxisSlave = 0 THEN
	GearIn := -2;
	RETURN;
END_IF

IF _AxisMaster = 0 THEN
	gearin := -1;
	RETURN;
END_IF

IF NOT _AxisSlave.isStationary() THEN
	Gearin := -3;
	RETURN;
END_IF

IF _AxisSlave.isCoupled() THEN
	Gearin := -4;
	RETURN;
END_IF

IF mcGearIn.Execute THEN
	Gearin := -5;
	RETURN;
END_IF

mcGearIn.Execute := TRUE;
]]></ST>
      </Implementation>
    </Method>
    <Method Name="GearOut" Id="{7cc81acd-0a8f-4d8a-a325-58c9914d04f1}">
      <Declaration><![CDATA[METHOD GearOut : HRESULT
VAR_INPUT
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF _AxisSlave = 0 THEN
	GearOut := -1;
	RETURN;
END_IF

IF _AxisMaster = 0 THEN
	GearOut := -2;
	RETURN;
END_IF

IF NOT _AxisSlave.isStationary() THEN
	GearOut := -3;
	RETURN;
END_IF

IF NOT _AxisSlave.isCoupled() THEN
	GearOut := -4;
	RETURN;
END_IF

IF mcGearOut.Execute THEN
	GearOut := -5;
	RETURN;
END_IF

mcGearOut.Execute := TRUE;]]></ST>
      </Implementation>
    </Method>
    <Property Name="InGear" Id="{7b9c5c0a-d5e8-485c-a913-d9dcff95acaf}">
      <Declaration><![CDATA[PROPERTY InGear : BOOL]]></Declaration>
      <Get Name="Get" Id="{90c2ce14-3a80-48f2-ac71-a190f78ea8d7}">
        <Declaration><![CDATA[VAR
END_VAR
]]></Declaration>
        <Implementation>
          <ST><![CDATA[InGear := mcGearIn.InGear AND _AxisSlave.isCoupled();]]></ST>
        </Implementation>
      </Get>
    </Property>
