    <Declaration><![CDATA[FUNCTION_BLOCK BufferedModuloBehaviour IMPLEMENTS I_Task
VAR_INPUT
END_VAR
VAR_OUTPUT
END_VAR
VAR
	moveMod : ARRAY[0..1] OF MoveMod;
	iMoveMod : I_MoveMod := moveMod[0];
	moveIndex : INT;
	_Axis : REFERENCE TO AXIS_REF;
	MoveToPosition : BOOL;
	State: INT;
	_Done: BOOL;
	_Error: BOOL;
	_Busy: BOOL;
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[{analysis -2}]]></ST>
    </Implementation>
    <Method Name="Abort" Id="{c7361e47-825a-4abf-80d8-120a1089eb55}">
      <Declaration><![CDATA[METHOD Abort : BOOL
]]></Declaration>
      <Implementation>
        <ST><![CDATA[iMoveMod.Execute := FALSE;
iMoveMod.Position := 0;
iMoveMod.Velocity := 0;
MoveToPosition := FALSE;
_Busy := FALSE;
Abort := TRUE;
State := 0;]]></ST>
      </Implementation>
    </Method>
