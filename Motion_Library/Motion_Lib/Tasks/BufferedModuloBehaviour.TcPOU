    <Method Name="Cycle" Id="{a8f35d36-fc74-4dc1-ba67-2c3da0b93a4d}">
      <Declaration><![CDATA[METHOD Cycle : HRESULT
VAR_INPUT
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[Cycle := S_OK;
CASE State OF
	0:
		IF iMoveMod.Busy THEN
			State := 10;
			MoveToPosition := FALSE;
		END_IF
		
	10:
		IF NOT iMoveMod.Busy THEN
			IF iMoveMod.Done THEN
				_Done := TRUE;
			ELSIF iMoveMod.Error THEN
				_Error := TRUE;
			END_IF
			_Busy := FALSE;
			iMoveMod.Execute := FALSE;
			State := 0;
			Cycle := S_OK;
		END_IF
	ELSE
		Cycle := E_FAIL;
END_CASE

moveMod[0](Axis := _Axis);
moveMod[1](Axis := _Axis);]]></ST>
      </Implementation>
    </Method>
