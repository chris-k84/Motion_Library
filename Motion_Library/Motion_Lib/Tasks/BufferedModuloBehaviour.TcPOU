    <Method Name="MoveTo" Id="{9207e690-5364-4b6a-bd08-6224adaefb6a}">
      <Declaration><![CDATA[METHOD MoveTo : BOOL
VAR_INPUT
	Position : MC_LREAL;
	Velocity : MC_LREAL;
	Task : REFERENCE TO I_Task;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF iMoveMod.Execute THEN
	Abort();
	IF moveIndex = 0 THEN
		moveIndex := 1;
	ELSE
		moveIndex := 0;
	END_IF
	iMoveMod := moveMod[moveIndex];
END_IF

iMoveMod.Position := Position;
iMoveMod.Velocity := Velocity;
MoveTo := TRUE;
Task := THIS^;

_Done := FALSE;
_Error := FALSE;
_Busy := TRUE;
iMoveMod.Execute := TRUE;]]></ST>
      </Implementation>
    </Method>
