    <Method Name="Cycle" Id="{968b2426-4712-4973-a44f-8ea7571f0394}">
      <Declaration><![CDATA[METHOD Cycle : HRESULT
VAR_INPUT
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[Cycle := S_OK;
CASE State OF
	0:
		IF MoveToPosition THEN
			Cycle := S_PENDING;
			_Done := FALSE;
			_Error := FALSE;
			_Busy := TRUE;
			iMoveAbs.Execute := TRUE;
			IF iMoveAbs.Busy THEN
				State := 10;
				MoveToPosition := FALSE;
			END_IF
		END_IF
		
	10:
		IF NOT iMoveAbs.Busy THEN
			IF iMoveAbs.Done THEN
				_Done := TRUE;
			ELSIF iMoveAbs.Error THEN
				_Error := TRUE;
			END_IF
			_Busy := FALSE;
			iMoveAbs.Execute := FALSE;
			State := 0;
			Cycle := S_OK;
		END_IF
	ELSE
		Cycle := E_FAIL;
END_CASE

moveAbs[0](Axis := _Axis);
moveAbs[1](Axis := _Axis);]]></ST>
      </Implementation>
    </Method>
