	MoveToPosition : BOOL;
	State: INT;
	_Done: BOOL;
	_Error: BOOL;
	_Busy: BOOL;
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[{analysis -2}]]></ST>
    </Implementation>
    <Method Name="Abort" Id="{9045061b-9962-4607-8807-e07e05404656}">
      <Declaration><![CDATA[METHOD Abort : BOOL
]]></Declaration>
      <Implementation>
        <ST><![CDATA[iMoveAbs.Execute := FALSE;
iMoveAbs.Position := 0;
iMoveAbs.Velocity := 0;
MoveToPosition := FALSE;
_Busy := FALSE;
Abort := TRUE;
State := 0;]]></ST>
      </Implementation>
    </Method>
